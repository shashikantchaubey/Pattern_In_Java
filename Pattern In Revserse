  *
*/

public class pattern_reverse {

    public static void main(String args[]){

        // int m =4;

        for(int i = 4; i>=1;i--){

            for(int j = 1; j<=i; j++){

                System.out.print("*");
            }
            System.out.println("");

        }
    }
    
}
